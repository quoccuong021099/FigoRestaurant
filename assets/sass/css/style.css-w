.btn {
  background-color: #8cB560;
  color: #fff;
  border-radius: 5px;
  border: none;
  outline: navajowhite;
  cursor: pointer;
  padding: 2.25rem 4rem;
  font-size: 1.6rem;
  font-weight: bold;
  font-family: "Poppins";
}

.btn:hover {
  background-color: #59882B;
}

@media only screen and (max-width: 1023px) {
  .navbar__btn {
    display: flex;
    justify-content: flex-end;
  }
}
.navbar__btn-icon {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 4rem;
  height: 4rem;
  padding: 1.2rem;
  border-radius: 50%;
  background-color: #fff;
  cursor: pointer;
}
.navbar__btn-icon:hover {
  background-color: #8cB560;
  color: #fff;
}
.navbar__btn .navbar__overlay {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 2;
  background-color: rgba(0, 0, 0, 0.3);
  display: none;
  animation: fadeIn linear 0.3s;
}
.navbar__btn .navbar__mobile-close {
  display: inline-block;
  width: 28px;
  height: 28px;
  color: #212529;
  position: absolute;
  top: 1rem;
  left: 2rem;
  cursor: pointer;
}
.navbar__btn .navbar__mobile-close:hover {
  color: #8cB560;
}
.navbar__btn .navbar__mobile {
  z-index: 2;
  position: fixed;
  top: 0;
  right: 0;
  bottom: 0;
  background-image: linear-gradient(to right bottom, #f8fbf5, #f7fdfa, #f9fdfe, #fdfeff, #ffffff);
  width: 480px;
  max-width: 100%;
  transform: translateX(100%);
  opacity: 0;
  transition: all linear 0.3s;
}
.navbar__btn .navbar__mobile .footer {
  padding: 0;
  position: absolute;
  bottom: 0;
}
.navbar__btn .navbar__mobile-list {
  margin-top: 24px;
}
.navbar__btn .navbar__mobile-link {
  text-align: center;
  color: #212529;
  display: block;
  font-size: 3rem;
  font-weight: 400;
  padding: 20px 40px;
}
.navbar__btn .navbar__mobile-link svg {
  width: 4rem;
  height: 4rem;
}
.navbar__btn .navbar__mobile-link:hover {
  color: #8cB560;
  fill: #8cB560;
}
.navbar__btn .navbar__input:checked ~ .navbar__overlay {
  display: block;
}
.navbar__btn .navbar__input:checked ~ .navbar__mobile {
  transform: translateX(0%);
  opacity: 1;
}
@keyframes fadeIn {
  from {
    opacity: 0;
  }
  to {
    opacity: 1;
  }
}

